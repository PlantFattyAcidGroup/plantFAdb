<% content_for :top_bar do %>
	<h1 class="pagetitle">Parameter: <%= @parameter.id %></h1>
<% end %>

<% content_for :item_bar do %>
	<%= link_to 'Back', parameters_path, class: 'button' %>
  <%= link_to 'Edit', edit_parameter_path(@parameter), class: 'button' %>
<% end %>

<p>
  <strong>Delta notation:</strong>
  <%= @parameter.delta_notation %>
</p>

<p>
  <strong>Cas number:</strong>
  <%= @parameter.cas_number %>
</p>

<p>
  <strong>Sofa mol:</strong>
  <%= @parameter.sofa_mol_id %>
</p>

<hr/>
<%= render partial: "measures/results", locals: {results: @parameter.results} %>


