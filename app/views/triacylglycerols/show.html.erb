<% content_for :top_bar do %>
	<h1 class="pagetitle">Triacylglycerol: <%= @triacylglycerol.id %></h1>
<% end %>

<% content_for :item_bar do %>
	<%= link_to 'Back', triacylglycerols_path, class: 'button' %>
  <%= link_to 'Edit', edit_triacylglycerol_path(@triacylglycerol), class: 'button' %>
<% end %>

<p>
  <strong>Delta notation:</strong>
  <%= @triacylglycerol.delta_notation %>
</p>

<p>
  <strong>Cas number:</strong>
  <%= @triacylglycerol.cas_number %>
</p>

<p>
  <strong>Sofa mol:</strong>
  <%= @triacylglycerol.sofa_mol_id %>
</p>

<hr/>
<%= render partial: "measures/results", locals: {results: @triacylglycerol.results} %>


